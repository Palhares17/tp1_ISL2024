#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x622e718414f0 .scope module, "TesteVerificacaoParidade" "TesteVerificacaoParidade" 2 3;
 .timescale 0 0;
v0x622e7188c900_0 .var "entrada", 5 0;
v0x622e7188c9f0_0 .net "paridade_valida", 0 0, v0x622e7188c720_0;  1 drivers
S_0x622e71841680 .scope module, "verificador" "VerificacaoParidade" 2 8, 3 1 0, S_0x622e718414f0;
 .timescale 0 0;
    .port_info 0 /INPUT 6 "entrada";
    .port_info 1 /OUTPUT 1 "paridade_valida";
v0x622e71841860_0 .net "entrada", 5 0, v0x622e7188c900_0;  1 drivers
v0x622e7188c720_0 .var "paridade_valida", 0 0;
v0x622e7188c7e0_0 .var/i "soma", 31 0;
E_0x622e7187bb70 .event anyedge, v0x622e71841860_0, v0x622e7188c7e0_0;
    .scope S_0x622e71841680;
T_0 ;
    %wait E_0x622e7187bb70;
    %load/vec4 v0x622e71841860_0;
    %parti/s 1, 4, 4;
    %pad/u 32;
    %load/vec4 v0x622e71841860_0;
    %parti/s 1, 3, 3;
    %pad/u 32;
    %add;
    %load/vec4 v0x622e71841860_0;
    %parti/s 1, 2, 3;
    %pad/u 32;
    %add;
    %load/vec4 v0x622e71841860_0;
    %parti/s 1, 1, 2;
    %pad/u 32;
    %add;
    %load/vec4 v0x622e71841860_0;
    %parti/s 1, 0, 2;
    %pad/u 32;
    %add;
    %store/vec4 v0x622e7188c7e0_0, 0, 32;
    %load/vec4 v0x622e7188c7e0_0;
    %pushi/vec4 2, 0, 32;
    %mod;
    %load/vec4 v0x622e71841860_0;
    %parti/s 1, 5, 4;
    %pad/u 32;
    %cmp/e;
    %flag_get/vec4 4;
    %store/vec4 v0x622e7188c720_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x622e718414f0;
T_1 ;
    %pushi/vec4 0, 0, 6;
    %store/vec4 v0x622e7188c900_0, 0, 6;
    %delay 10, 0;
    %load/vec4 v0x622e7188c9f0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_1.0, 4;
    %vpi_call 2 17 "$display", "2: 1101101" {0 0 0};
    %jmp T_1.1;
T_1.0 ;
    %vpi_call 2 19 "$display", "E: 1111011" {0 0 0};
T_1.1 ;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench_paridade.v";
